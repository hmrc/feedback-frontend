{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "tasks": [{
    "args": ["clean"],
    "command": "sbt",
    "label": "sbt clean",
    "options": {
      "cwd": "${workspaceFolder}"
    },
    "type": "shell"
  }, {
    "args": ["compile"],
    "command": "sbt",
    "label": "sbt compile",
    "options": {
      "cwd": "${workspaceFolder}"
    },
    "type": "shell"
  }, {
    "args": ["evicted"],
    "command": "sbt",
    "label": "sbt evicted",
    "options": {
      "cwd": "${workspaceFolder}"
    },
    "type": "shell"
  }, {
    "args": ["run", "--mem", "6699"],
    "command": "sbt",
    "label": "sbt run",
    "options": {
      "cwd": "${workspaceFolder}"
    },
    "type": "shell"
  }, {
    "args": ["test", "--mem", "6699"],
    "command": "sbt",
    "label": "sbt test",
    "options": {
      "cwd": "${workspaceFolder}"
    },
    "type": "shell"
  }],
  "version": "2.0.0"
}
